	<script type="text/javascript">
		function userLogout() {
			alertify.notify('Logging out', 'custom', 2, function () { console.log('dismissed'); });
			$.ajax({
				type: 'GET',
				url: '/User/Logout',
				success: function (r) {
					alertify.notify('You have Logged out' + category, 'success', 3, function () { console.log('dismissed'); });
				},
				error: function (e) {
					alertify.error('Error: JavaScript Function (userLogout)');
				}
			});
			window.location.href = '/Shop/Index';
		}
		$(document).ready(function () {
			$('#LoginNormal').show();
			$('#UserInfoDisplay').show();
			$('#RegisterNormal').hide();
			$('#UserUpdateContact').hide(); 
			$('#UserUpdateShipping').hide(); 
		});
		function showRegisterNormal() {
			$('#RegisterNormal').show();
			$('#LoginNormal').hide();
		}
		function showLoginNormal() {
			$('#LoginNormal').show();							
			$('#RegisterNormal').hide();			
		}
		function showRejectStatic() {
			window.location.href = '/User/Index';
		}
		function showUserInfoDisplay() {
			$('#UserInfoDisplay').show();
			$('#UserInfoEdit').hide();
		}
		function showUserUpdateContact() {
			$('#UserUpdateContact').show();
			$('#UserInfoDisplay').hide();
		}
		function showUserUpdateShipping() {
			$('#UserUpdateShipping').show();
			$('#UserInfoDisplay').hide();
		}
	</script>
