@using WebScrapper_Prototype.Controllers;
@using Microsoft.AspNetCore.Identity;
@using WebScrapper_Prototype.Areas.Identity.Data;

@inject SignInManager<ApplicationUser> SignInManager
@inject UserManager<ApplicationUser> UserManager
@model WebScrapper_Prototype.Models.UserViewModel
@{
	ViewData["Title"] = "Account Details";
}
<!DOCTYPE html>
<html lang="zxx">
<head>
	<!-- Title Tag  -->
	<title>Account Details</title>
	<!-- Meta Tag -->
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name='copyright' content=''>
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
	<!-- Favicon -->
	<link rel="icon" type="image/png" href="/Media/LogoCropped.png" src="/Media/LogoCropped.png">
	<!-- Web Font -->
	<link href="https://fonts.googleapis.com/css?family=Lato:200i,300,300i,400,400i,500,500i,600,600i,700,700i,800,800i,900,900i&display=swap" rel="stylesheet">
	<!-- StyleSheet -->
	<!-- Bootstrap -->
	<link rel="stylesheet" href="~/css/bootstrap.css">
	<!-- Magnific Popup -->
	<link rel="stylesheet" href="~/css/magnific-popup.min.css">
	<!-- Font Awesome -->
	<link rel="stylesheet" href="~/css/font-awesome.css">
	<!-- Fancybox -->
	<link rel="stylesheet" href="~/css/jquery.fancybox.min.css">
	<!-- Themify Icons -->
	<link rel="stylesheet" href="~/css/themify-icons.css">
	<!-- Jquery Ui -->
	<link rel="stylesheet" href="~/css/jquery-ui.css">
	<!-- Nice Select CSS -->
	<link rel="stylesheet" href="~/css/niceselect.css">
	<!-- Animate CSS -->
	<link rel="stylesheet" href="~/css/animate.css">
	<!-- Flex Slider CSS -->
	<link rel="stylesheet" href="~/css/flex-slider.min.css">
	<!-- Owl Carousel -->
	<link rel="stylesheet" href="~/css/owl-carousel.css">
	<!-- Slicknav -->
	<link rel="stylesheet" href="~/css/slicknav.min.css">
	<!-- Alertify -->
	<link rel="stylesheet" href="~/alertifyjs/css/alertify.min.css" />
	<link rel="stylesheet" href="~/alertifyjs/css/themes/default.min.css" />
	<!-- Reset CSS -->
	<link rel="stylesheet" href="~/css/reset.css">
	<!-- Nice Select JS -->
	<link rel="stylesheet" href="~/css/niceselect.css">
	<!-- Temp CSS -->
	<link rel="stylesheet" href="~/css/style.css">
	<!-- Responsive CSS -->
	<link rel="stylesheet" href="~/css/responsive.css">
	<!-- Site CSS -->
	<link rel="stylesheet" href="~/css/site.css">
	<style>
		.section-divider {
			border-top: 1px solid black;
			padding-top: 20px;
			margin-top: 20px;
		}

		.div-divider {
			border-right: 1px solid black;
			padding-right: 10px;
			margin-right: 10px;
		}

	</style>
</head>
<body class="js">
	<!-- Preloader -->
	<div class="preloader">
		<div class="preloader-inner">
			<div class="preloader-icon">
				<span></span>
				<span></span>
			</div>
		</div>
	</div>
	<!-- End Preloader -->
	<!-- End Scroll -->
	<!-- Header -->
	<div class="header shop">
		<!-- Topbar -->
		<div class="topbar">
			<div class="container">
				<div class="row">
					<div class="col-lg-5 col-md-12 col-12">
						<!-- Top Left -->
						<div class="top-left">
							<ul class="list-main">
								<li><i class="ti-headphone-alt"></i> <a href="tel:0815650206">Call us</a></li>
								<li><i class="ti-email"></i> <a href="mailto:brandenconnected@gmail.com">Email us</a></li>
							</ul>
						</div>
						<!--/ End Top Left -->
					</div>
					<div class="col-lg-6 col-md-12 col-12">
						<!-- Top Right -->
						<div class="right-content">
							@if (User.IsInRole("Manager"))
							{
								<ul class="list-main">
									<li><i class="ti-shopping-cart"></i><a href="/Shop/Cart">Cart</a></li>
									<li><i class="ti-bag"></i><a href="#products">Shop</a></li>
									<li><i class="ti-dashboard"></i><a href="/Backend">Backend</a></li>
									<li><i class="ti-power-off"></i><a asp-area="Identity" asp-page="/Account/Logout" asp-route-returnUrl="@Url.Action("Index", "Home" , new { area="" })">Logout</a></li>
								</ul>
							}
							else if (User.IsInRole("User"))
							{
								<ul class="list-main">
									<li><i class="ti-shopping-cart"></i><a href="/Shop/Cart">Cart</a></li>
									<li><i class="ti-bag"></i><a href="#products">Shop</a></li>
								</ul>
							}
							else
							{
								<ul class="list-main">
									<li><i class="ti-shopping-cart"></i><a href="/Shop/Cart">Cart</a></li>
									<li><i class="ti-bag"></i><a href="/Shop">Shop</a></li>
									<li><i class="ti-power-off"></i><a href="/User">Sign up</a></li>
								</ul>
							}
						</div>
						<!-- End Top Right -->
					</div>
				</div>
			</div>
		</div>
		<!-- End Topbar -->
		<!-- Middle Inner -->
		<div class="middle-inner">
			<div class="container">
				<div class="row">
					<div class="col-lg-2 col-md-2 col-12">
						<!-- Logo -->
						<div class="logo">
							<a href="/"><img src="/Media/LogoCropped.png" alt="logo"></a>
						</div>
						<!--/ End Logo -->
						<!-- Search Form -->
						<div class="search-top">
							<div class="top-search"><a href="#"><i class="ti-search"></i></a></div>
							<!-- Search Form -->
							<div class="search-top">
								<form class="search-form">
									@Html.TextBox("SearchString", ViewBag.CurrentFilter as string,
									new {Class="form-control me-2", PlaceHolder="Search Products..."})
									<button value="search" type="submit"><i class="ti-search"></i></button>
								</form>
							</div>
							<!--/ End Search Form -->
						</div>
						<!--/ End Search Form -->
						<div class="mobile-nav"></div>
					</div>
					<div class="col-lg-8 col-md-7 col-12">
						<div class="search-bar-top">
							<div class="search-bar">
								<form>
									@Html.TextBox("SearchString", ViewBag.CurrentFilter as string,
									new {Class="form-control me-2", PlaceHolder="Search..."})
									<button class="btnn" type="submit"><i class="ti-search"></i></button>
								</form>
							</div>
						</div>
					</div>
					<div class="col-lg-2 col-md-3 col-12">
						<div class="right-bar">
							<div class="sinlge-bar shopping">
								<a id="AccountHover" href="#" onclick="loadAccount()" class="single-icon"><i class="fa fa-user-circle-o"></i></a>
								@if (ViewBag.UserLayer != 0)
								{
									<div id="AccountOptions" class="shopping-item">
										<div class="dropdown-cart-header">
											<p class="h4">@ViewBag.FirstName</p>
										</div>
										<ul class="shopping-list">
											<li>
												<a class="h5" href="/User">Login</a>
											</li>
											<li>
												<a class="h5" href="/User">Register</a>
											</li>
										</ul>
									</div>
								}
								else
								{
									<div id="AccountOptions" class="shopping-item">
										<div class="dropdown-cart-header">
											<p>Hello <span class="font-weight-bold">@Model.FirstName</span>,<br /> What would you like to do?</p>
										</div>
										<ul class="shopping-list">
											<li>
												<a class="cart-img" href="/User"><img src="#" alt="UserImage" /></a>
											</li>
											<li>
												<a class="h4" href="/User">My Account</a>
											</li>
											<li>
												<a class="h5" href="/User">My Orders</a>
											</li>
											<li>
												<a class="h6" href="/User">Check Order Status</a>
											</li>
										</ul>
										<div class="bottom">
											<a href="#" onclick="userLogout()" class="btn animate">Logout</a>
										</div>
									</div>
								}
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
		<!--/ End Middle Inner -->
		<!-- Header Inner -->
		<div class="header-inner">
			<div class="container">
				<div class="cat-nav-head">
					<div class="row">
						<div class="col-12">
							<div id="shopmenu" class="menu-area">
								<!-- Main Menu -->
								<nav class="navbar navbar-expand-lg">
									<div class="navbar-collapse">
										<div class="nav-inner">
											<ul class="nav main-menu menu navbar-nav">
												<li class="active">
													<a href="/Shop">Shop</a>
												</li>
												<li>
													<a>Upgrade Kits & Combos<i class="ti-angle-down"></i></a>
													<ul class="dropdown">
														<li><a href="/Shop">Upgrade Kits</a></li>
														<li><a href="/Shop">CPUs & Coolers Combos</a></li>
														<li><a href="/Shop">Peripherals Combos</a></li>
													</ul>
												</li>
												<li>
													<a>Trending Products<i class="ti-angle-down"></i><span class="new">New</span></a>
													<ul class="dropdown">
														<li><a href="/Shop">Intels Latest i7 & i5 CPUs</a></li>
														<li><a href="/Shop">AMD Ryzen</a></li>
														<li><a href="/Shop">Nvidia RTX</a></li>
														<li><a href="/Shop">Gaming Peripherals & Accessories</a></li>
													</ul>
												</li>
											</ul>
										</div>
									</div>
								</nav>
								<!--/ End Main Menu -->
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
		<!--/ End Header Inner -->
	</div>
	@if (ViewBag.UserLayer == 0)
	{
		<section id="UserInfoDisplay" class="shop checkout section">
			<div class="container">
				<div class="row section-divider">
					<div class="col-9">
						<br>
						<p class="fs-4 h4">Welcome back,</p>
						<p class="fs-2 h2">@Model.FirstName @Model.LastName</p>
					</div>
					<div class="col-4 div-divider" style="margin: 0 10px;">
						<p class="fw-bolder h5"><br>Contact Details <a href="#" onclick="showUserUpdateContact()"><i class="ti-pencil-alt" style="white-space: nowrap;" data-toggle="tooltip" data-placement="right" title="Change Contact Details"></i></a></p>
						<div style="top:15px" class="row">
							<div class="col">
								<p class="label-info fs-4 fw-bolder">First Name</p>
								<p class="fs-4 h6 fw-bolder" style="top:0;">@Model.FirstName</p>
							</div>
							<div class="col">
								<p class="label-info fs-4 fw-bolder">Last Name</p>
								<p class="fs-4 h6 fw-bolder" style="top:0;">@Model.LastName</p>
							</div>
						</div>
						<div style="top:15px" class="row">
							<div class="col">
								<p class="label-info fs-4 fw-bolder">Email Address</p>
								<p class="fs-4 h6 fw-bolder" style="top:0;">@Model.Email</p>
							</div>
						</div>
						<div style="top:15px" class="row">
							<div class="col">
								<p class="label-info fs-4 fw-bolder">Phone Number</p>
								<p class="fs-4 h6 fw-bolder" style="top:0;">@Model.PhoneNumber</p>
							</div>
							<div class="col">
								<p class="label-info fs-4 fw-bolder">Password <a href="#" onclick=""><i class="ti-pencil-alt" style="white-space: nowrap;" data-toggle="tooltip" data-placement="right" title="Change Password"></i></a></p>
								<p class="fs-4 h6 fw-bolder" style="top:0;">********</p>
							</div>
						</div>
					</div>
					<div class="col-6 div-divider">
						<div class="col-lg-8 col-12">
							<p class="fw-bolder h5"><br>Shipping Address <a href="#" onclick="showUserUpdateShipping()"><i class="ti-pencil-alt" style="white-space: nowrap;" data-toggle="tooltip" data-placement="right" title="Change Shipping Address"></i></a></p>
							<div style="top:15px" class="row">
								<div class="col-lg-6 col-md-6 col-12">
									<p class="fs-4 h6 fw-bolder" style="top:0;">@Model.Unit @Model.Street</p>
									<p class="fs-4 h6 fw-bolder" style="top:5px;">@Model.Area</p>
									<p class="fs-4 h6 fw-bolder" style="top:5px;">@Model.City @Model.PostalCode</p>
									<p class="fs-4 h6 fw-bolder" style="top:5px;">@Model.Province</p>
								</div>
							</div>
						</div>
					</div>
				</div>
				<div class="row" style="margin-top:10px;">
					<div class="col-9">
						<a href="#" class="button btn-outline-danger" style="padding:10px;">Delete Account</a>
					</div>
				</div>
			</div>
		</section>
		<section id="UserUpdateContact" class="shop checkout section">
			<div class="container">
				<div class="row section-divider">
					<div class="col-9">
						<br>
						<p class="fs-4 h4">Verify & Update Account</p>
						<p class="fs-2 h2">Contact Details</p>
					</div>
					<div class="col-9" style="margin: 0 10px;">
						<form asp-action="UserUpdateContact" class="form" method="post">
							<div asp-validation-summary="ModelOnly" class="text-danger"></div>
							<div class="row" style="top:15px">
								<div class="col-lg-6 col-md-6 col-12">
									<div class="form-group">
										<label asp-for="FirstName" class="form-label">First Name</label>
										<input asp-for="FirstName" type="text" name="firstName" class="form-control" aria-label="FirstName">
									</div>
								</div>
								<div class="col-lg-6 col-md-6 col-12">
									<div class="form-group">
										<label asp-for="LastName" class="form-label">Last Name</label>
										<input asp-for="LastName" type="text" name="lastName" value="@Model.LastName" class="form-control" aria-label="LastName">
									</div>
								</div>
							</div>
							<div class="row" style="top:15px">
								<div class="col-lg-6 col-md-6 col-12">
									<div class="form-group">
										<label asp-for="Email" class="form-label">Email Address</label> <i class="ti-info-alt" style="white-space: nowrap;" data-toggle="tooltip" data-placement="right" title="Want to use a different Email Address? Please Register a New Account using the different Email Address."></i>
										<input asp-for="Email" type="email" name="email" value="@Model.Email" class="form-control" aria-label="Email" readonly>
									</div>
								</div>
								<div class="col-lg-6 col-md-6 col-12">
									<div class="form-group">
										<label asp-for="PhoneNumber" class="form-label">Phone Number</label>
										<input asp-for="PhoneNumber" type="tel" name="phoneNumber" value="@Model.PhoneNumber" class="form-control" aria-label="PhoneNumber">
									</div>
								</div>
							</div>
							<div class="row">
								<div class="col" style="margin:20px 0;">
									<input type="submit" value="Save Changes" class="btn btn-primary" />
								</div>
							</div>
						</form>
					</div>
				</div>
			</div>
		</section>
		<section id="UserUpdateShipping" class="shop checkout section">
			<div class="container">
				<div class="row section-divider">
					<div class="col-9">
						<br>
						<p class="fs-4 h4">Verify & Update Account</p>
						<p class="fs-2 h2">Shipping Address</p>
					</div>
					<div class="col-9" style="margin: 0 10px;">
						<form asp-action="UserUpdateShipping" class="form" method="post">
							<div asp-validation-summary="ModelOnly" class="text-danger"></div>
							<div class="row" style="top:15px">
								<div class="col-lg-6 col-md-6 col-12">
									<div class="form-group">
										<label asp-for="Province" class="form-label">Province</label>
										<select asp-for="Province" name="province">
											<option selected="selected" value="@Model.Province">@Model.Province</option>
											<option value="WC">Western Cape</option>
											<option value="EC">Eastern Cape</option>
										</select>
									</div>
								</div>
								<div class="col-lg-6 col-md-6 col-12">
									<div class="form-group">
										<label asp-for="City" class="form-label">City/Town</label>
										<select asp-for="Province" name="province">
											<option selected="selected" value="@Model.City">@Model.City</option>
											<option value="CPT">Cape Town</option>
											<option value="PE">Port Elizabeth</option>
										</select>
									</div>
								</div>
							</div>
							<div class="row" style="top:15px">
								<div class="col-lg-6 col-md-6 col-12">
									<div class="form-group">

										<label asp-for="Unit" class="form-label">Unit No.</label>
										<input asp-for="Unit" type="text" name="unit" value="@Model.Unit" class="form-control" aria-label="unit">
									</div>
								</div>
								<div class="col-lg-6 col-md-6 col-12">
									<div class="form-group">
										<label asp-for="Street" class="form-label">Street Name</label>
										<input asp-for="Street" type="text" name="street" value="@Model.Street" class="form-control" aria-label="StreetName">
									</div>
								</div>
							</div>
							<div class="row" style="top:15px">
								<div class="col-lg-6 col-md-6 col-12">
									<div class="form-group">
										<label asp-for="Area" class="form-label">Area/Suburb</label>
										<input asp-for="Area" type="text" name="area" value="@Model.Area" class="form-control" aria-label="area">
									</div>
								</div>
								<div class="col-lg-6 col-md-6 col-12">
									<div class="form-group">
										<label asp-for="PostalCode" class="form-label">Street Name</label>
										<input asp-for="PostalCode" type="text" name="postalCode" value="@Model.PostalCode" class="form-control" aria-label="PostalCode">
									</div>
								</div>
							</div>
							<div class="row">
								<div class="col">
									<input type="submit" value="Save" class="btn btn-primary" />
								</div>
							</div>
						</form>
					</div>
				</div>
			</div>
		</section>
	}
	else if (ViewBag.UserLayer == 1)
	{
		<section id="LoginNormal" class="shop checkout section">
			<div class="container">
				<div class="row section-divider">
					<div class="col-9">
						<br>
						<p class="fs-4 h4">Stop wasting your time looking for discounts & deals. We got you covered!</p>
						<p class="fs-2 h2">Please Login into your <span style="color:orange; font-weight:800;">Wa</span><span style="color:forestgreen; font-weight:800;">Za</span><span style="color:orange; font-weight:800;">Ware</span> Account</p>
					</div>
					<div class="col-9" style="margin: 0 10px;">
						<p class="fw-bolder h5"><br>Login</p>
						<form asp-action="Login" class="form" method="post">
							<div class="row" style="top:15px">
								<div class="col-lg-6 col-md-6 col-12">
									<div class="form-group">
										<label asp-for="Email" class="form-label">Email:</label>
										<input asp-for="Email" type="email" name="email" autocomplete="on" placeholder="lowprices@wazaware.co.za" class="form-control" aria-label="Email">
									</div>
								</div>
							</div>
							<div class="row" style="top:15px">
								<div class="col-lg-6 col-md-6 col-12">
									<div class="form-group">
										<label asp-for="Password" class="form-label">Password</label>
										<input asp-for="Password" type="password" name="password" autocomplete="on" class="form-control" placeholder="Password" aria-label="Password">
										<a href="#"><i class="ti-angle-right" style="white-space: nowrap;" data-toggle="tooltip" data-placement="right" title="Reset Password"> Forgot Password?</i></a>
									</div>
								</div>
							</div>
							<div class="row">
								<div class="col">
									<input type="submit" value="Login" class="btn btn-primary" />
								</div>
							</div>
						</form>
						<div class="row">
							<div class="col" style="top:10px;">
								<button onclick="showRegisterNormal()" type="button" class="btn-outline-info" style="padding:5px;">Not Registered?</button>
							</div>
						</div>
					</div>
				</div>
			</div>
		</section>
		<section id="RegisterNormal" class="shop checkout section">
			<div class="container">
				<div class="row section-divider">
					<div class="col-9">
						<br>
						<p class="fs-4 h4">Unlock Unbelievable Discounts All Day, Everyday!</p>
						<p class="fs-2 h2">Please Complete the Form</p>
					</div>
					<div class="col-9" style="margin: 0 10px;">
						<p class="fw-bolder h5"><br>We don't share any of your information with anyone</p>
						<form id="registerForm" asp-action="Register" class="form" method="post">
							<div asp-validation-summary="ModelOnly" class="text-danger"></div>
							<div class="row" style="top:15px">
								<div class="col-lg-6 col-md-6 col-12">
									<div class="form-group">
										<label asp-for="FirstName" class="form-label">First Name</label>
										<input asp-for="FirstName" type="text" name="firstName" autocomplete="on" placeholder="Bill" class="form-control" aria-label="FirstName">
									</div>
								</div>
								<div class="col-lg-6 col-md-6 col-12">
									<div class="form-group">
										<label asp-for="LastName" class="form-label">Last Name</label>
										<input asp-for="LastName" type="text" name="lastName" autocomplete="on" placeholder="Gates" class="form-control" aria-label="LastName">
									</div>
								</div>
							</div>
							<div class="row" style="top:15px">
								<div class="col-lg-6 col-md-6 col-12">
									<div class="form-group">
										<label asp-for="Email" class="form-label">Email Address</label>
										<input asp-for="Email" type="email" name="email" autocomplete="on" placeholder="lowprices@wazaware.co.za" class="form-control" aria-label="Email">
									</div>
								</div>
								<div class="col-lg-6 col-md-6 col-12">
									<div class="form-group">
										<label asp-for="PhoneNumber" class="form-label">Phone Number</label>
										<input asp-for="PhoneNumber" type="tel" name="phoneNumber" autocomplete="on" placeholder="0815650206" class="form-control" aria-label="PhoneNumber">
									</div>
								</div>
							</div>
							<div class="row" style="top:15px">
								<div class="col-lg-6 col-md-6 col-12">
									<div class="form-group">
										<label asp-for="Password" class="form-label">Password</label>
										<input asp-for="Password" type="Password" name="password" autocomplete="off" placeholder="Password" class="form-control" aria-label="Password">
									</div>
								</div>
								<div class="col-lg-6 col-md-6 col-12">
									<div class="form-group">
										<label asp-for="ConfirmPassword" class="form-label">Confirm Password</label>
										<input asp-for="ConfirmPassword" type="password" name="confirmPassword" autocomplete="off" placeholder="Confirm Password" class="form-control" aria-label="ConfirmPassword">
									</div>
								</div>
							</div>
							<div class="row">
								<div class="col">
									<input type="submit" value="Register" class="btn btn-primary" />
								</div>
							</div>
						</form>
						<div class="row" style="top:15px">
							<div class="col">
								<button onclick="showLoginNormal()" type="button" class="btn-outline-info" style="padding:5px;">Already Registered?</button>
							</div>
						</div>
					</div>
				</div>
			</div>
		</section>
	}
	else if (ViewBag.UserLayer == 2)
	{
		<section id="LoginStatic" class="shop checkout section">
			<div class="container">
				<div class="row section-divider">
					<div class="col-9">
						<br>
						<p class="fs-4 h4">Oops, looks like you trying to create an account with an email that is already registered...</p>
						<p class="fs-2 h2">Try Login with @ViewBag.Result</p>
					</div>
					<div class="col-9">
						<p class="fw-bolder h5"><br>Login</p>
						<form asp-action="Login" class="form" method="post">
							<div asp-validation-summary="ModelOnly" class="text-danger"></div>
							<div class="row" style="top:15px">
								<div class="col-lg-6 col-md-6 col-12">
									<div class="form-group">
										<label asp-for="Email" class="form-label">Email</label>
										<input asp-for="Email" type="email" name="email" value="@ViewBag.Result" class="form-control" aria-label="Email" readonly>
									</div>
								</div>
							</div>
							<div class="row" style="top:15px">
								<div class="col-lg-6 col-md-6 col-12">
									<div class="form-group">
										<label asp-for="Password" class="form-label">Password</label>
										<input asp-for="Password" type="password" name="password" autocomplete="on" class="form-control" placeholder="Password" aria-label="Password">
										<a href="#"><i class="ti-angle-right" style="white-space: nowrap;" data-toggle="tooltip" data-placement="right" title="Reset Password"> Forgot Password?</i></a>
									</div>
								</div>
							</div>
							<div class="row">
								<div class="col">
									<input type="submit" value="Login" class="btn btn-primary" />
								</div>
							</div>
							<div class="row" style="top:15px">
								<div class="col-lg-6 col-md-6 col-12">
									<div class="form-group">
										<button onclick="showRejectStatic()" type="button" class="button btn-outline-info" style="padding:5px;">Use a Different Email?</button>
									</div>
								</div>
							</div>
						</form>
					</div>
				</div>
			</div>
		</section>
	}

