	<script type="text/javascript">
		$(document).on("click", ".filter-link", function (e) {
			e.preventDefault();

			var filter = $(this).data("filter");

			// Make an AJAX call to retrieve filtered products
			$.ajax({
				type: "GET",
				url: "/Home/Index",
				data: { filter: filter },
				success: function (result) {
					// Update only the content of the element with class "tab-content"
					$("#products .tab-content").html($(result).find("#products .tab-content").html());
				},
				error: function (xhr, status, error) {
					console.error(error);
				}
			});
		});
	</script>
